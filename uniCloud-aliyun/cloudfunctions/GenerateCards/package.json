
{
  "name": "GenerateCards",
  "version": "1.0.0",
  "description": "定时生成卡片",
  "cloudfunction-config": {
    "concurrency": 1,          
    "memorySize": 256,         
    "timeout": 600,            
    "triggers": [
      {
        "name": "generateCardsTrigger",
        "type": "timer",
        "config": "0 0 0 * * * *"  
      }
    ]
  }
}